{
  "name": "music-class-app",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pascalvaccaro/strapi-svelte.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "scripts": {
    "build:admin": "(cd admin && yarn build)",
    "build:app": "(cd app && yarn build)",
    "build": "concurrently \"yarn build:admin\" \"yarn build:app\"",
    "setup": "yarn install && concurrently \"yarn setup:admin\" \"yarn setup:app\"",
    "setup:admin": "(cd admin && yarn install)",
    "setup:app": "(cd app && yarn install)",
    "dev": "concurrently \"yarn dev:admin\" \"yarn dev:app\"",
    "dev:admin": "(cd admin && yarn develop)",
    "dev:app": "(cd app && yarn dev)"
  },
  "dependencies": {
    "concurrently": "^8.2.2"
  }
}
